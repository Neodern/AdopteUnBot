{
    "name": "AdopteUnBot",
    "version": "0.1.1",
    "description": "Visit automaticaly all profils from Homepage & research !",
    "icons": {
    "16" : "medias/images/adopte-logo.png",
    "48" : "medias/images/adopte-logo.png",
    "128" : "medias/images/adopte-logo.png"
    },
    "manifest_version": 2,
    "default_locale": "fr",
    "content_scripts": [ {
        "matches": [
            "*://www.adopteunmec.com/*"
        ],
        "js": [
            "medias/jQuery/jquery-2.0.2.min.js",
            "medias/jQuery/jquery-timing.js",
            "aub.js"
        ],
        "css": [
            "medias/css/adopteunbot.css"
        ]
    }],

    "permissions": [
        "tabs",
        "*://www.adopteunmec.com/*"
    ],

    "web_accessible_resources": [
        "medias/jQuery/jquery-2.0.3.min.map",
        "medias/images/green-icon.png",
        "medias/images/search_bot.png",
        "medias/images/search_top.png"
    ]
}